{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [true, {
            "multiline": "never",
            "singleline": "never"
        }],
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-namespace": [true, "allow-declarations"],
        "no-var-requires": false
    },
    "rulesDirectory": []
}